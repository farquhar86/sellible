<p>Sign In Admin</p>    
<%= form_for @superuser, url: "/sessions", method: "post", :html => {:class => "loginForm"} do |f| %>
    <div>
        <%= f.text_field :email, placeholder: "Email" %>
    </div>
    <div>
        <%= f.password_field :password, placeholder: "Password" %>
    </div>
    
    <%= f.submit "Sign In"%>
<% end %>